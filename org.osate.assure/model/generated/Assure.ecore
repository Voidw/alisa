<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="assure" nsURI="http://www.osate.org/assure/Assure" nsPrefix="assure">
  <eClassifiers xsi:type="ecore:EClass" name="AssuranceEvidence" eSuperTypes="#//AssureResult">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType platform:/plugin/org.eclipse.emf.ecore/model/Ecore.ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="target" eType="ecore:EClass platform:/resource/org.osate.aadl2/model/aadl2.ecore#//Classifier"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="instance" eType="ecore:EClass platform:/resource/org.osate.aadl2/model/instance.ecore#//InstanceObject"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="message" eType="ecore:EDataType platform:/plugin/org.eclipse.emf.ecore/model/Ecore.ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="subAssuranceEvidence" upperBound="-1"
        eType="#//AssuranceEvidence" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="claimResult" upperBound="-1"
        eType="#//ClaimResult" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ClaimResult" eSuperTypes="#//AssureResult">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType platform:/plugin/org.eclipse.emf.ecore/model/Ecore.ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="target" eType="ecore:EClass platform:/resource/org.osate.reqspec/model/generated/ReqSpec.ecore#//Requirement"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="instance" eType="ecore:EClass platform:/resource/org.osate.aadl2/model/instance.ecore#//InstanceObject"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="message" eType="ecore:EDataType platform:/plugin/org.eclipse.emf.ecore/model/Ecore.ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="subClaimResult" upperBound="-1"
        eType="#//ClaimResult" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="verificationActivityResult"
        upperBound="-1" eType="#//VerificationExpr" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="VerificationResult" eSuperTypes="#//AssureResult #//VerificationExpr">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType platform:/plugin/org.eclipse.emf.ecore/model/Ecore.ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="executionState" eType="#//VerificationExecutionState"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="resultState" eType="#//VerificationResultState"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="issues" upperBound="-1"
        eType="#//ResultIssue" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="resultReport" eType="ecore:EClass platform:/resource/org.osate.results/model/generated/Results.ecore#//ResultReport"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="message" eType="ecore:EDataType platform:/plugin/org.eclipse.emf.ecore/model/Ecore.ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="AssureResult">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="successCount" eType="ecore:EDataType platform:/plugin/org.eclipse.emf.ecore/model/Ecore.ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="failCount" eType="ecore:EDataType platform:/plugin/org.eclipse.emf.ecore/model/Ecore.ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="unknownCount" eType="ecore:EDataType platform:/plugin/org.eclipse.emf.ecore/model/Ecore.ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="failthenCount" eType="ecore:EDataType platform:/plugin/org.eclipse.emf.ecore/model/Ecore.ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="andthenCount" eType="ecore:EDataType platform:/plugin/org.eclipse.emf.ecore/model/Ecore.ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="tbdCount" eType="ecore:EDataType platform:/plugin/org.eclipse.emf.ecore/model/Ecore.ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="weight" eType="ecore:EDataType platform:/plugin/org.eclipse.emf.ecore/model/Ecore.ecore#//EInt"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="VerificationExpr" eSuperTypes="#//AssureResult"/>
  <eClassifiers xsi:type="ecore:EClass" name="FailThenResult" eSuperTypes="#//VerificationExpr">
    <eStructuralFeatures xsi:type="ecore:EReference" name="first" upperBound="-1"
        eType="#//VerificationExpr" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="second" upperBound="-1"
        eType="#//VerificationExpr" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="failThen" eType="ecore:EDataType platform:/plugin/org.eclipse.emf.ecore/model/Ecore.ecore#//EBoolean"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="unknownThen" eType="ecore:EDataType platform:/plugin/org.eclipse.emf.ecore/model/Ecore.ecore#//EBoolean"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="didFail" eType="ecore:EDataType platform:/plugin/org.eclipse.emf.ecore/model/Ecore.ecore#//EBoolean"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="AndThenResult" eSuperTypes="#//VerificationExpr">
    <eStructuralFeatures xsi:type="ecore:EReference" name="first" upperBound="-1"
        eType="#//VerificationExpr" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="second" upperBound="-1"
        eType="#//VerificationExpr" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="didAndThenFail" eType="ecore:EDataType platform:/plugin/org.eclipse.emf.ecore/model/Ecore.ecore#//EBoolean"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ResultIssue">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="issueType" eType="#//ResultIssueType"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType platform:/plugin/org.eclipse.emf.ecore/model/Ecore.ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="message" eType="ecore:EDataType platform:/plugin/org.eclipse.emf.ecore/model/Ecore.ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="exceptionType" eType="ecore:EDataType platform:/plugin/org.eclipse.emf.ecore/model/Ecore.ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="target" eType="ecore:EClass platform:/plugin/org.eclipse.emf.ecore/model/Ecore.ecore#//EObject"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="issues" upperBound="-1"
        eType="#//ResultIssue" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="ResultIssueType">
    <eLiterals name="ERROR" literal="error"/>
    <eLiterals name="WARNING" value="1" literal="warning"/>
    <eLiterals name="SUCCESS" value="2" literal="success"/>
    <eLiterals name="INFO" value="3" literal="info"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="VerificationResultState">
    <eLiterals name="TBD" literal="tbd"/>
    <eLiterals name="SUCCESS" value="1" literal="success"/>
    <eLiterals name="FAIL" value="2" literal="fail"/>
    <eLiterals name="UNKNOWN" value="3" literal="unknown"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="VerificationExecutionState">
    <eLiterals name="TODO" literal="todo"/>
    <eLiterals name="RUNNING" value="1" literal="running"/>
    <eLiterals name="REDO" value="2" literal="redo"/>
    <eLiterals name="COMPLETED" value="3" literal="completed"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ValidationResult" eSuperTypes="#//VerificationResult">
    <eStructuralFeatures xsi:type="ecore:EReference" name="target" eType="ecore:EClass platform:/resource/org.osate.verify/model/generated/Verify.ecore#//VerificationValidation"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="PreconditionResult" eSuperTypes="#//VerificationResult">
    <eStructuralFeatures xsi:type="ecore:EReference" name="target" eType="ecore:EClass platform:/resource/org.osate.verify/model/generated/Verify.ecore#//VerificationPrecondition"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="VerificationActivityResult" eSuperTypes="#//VerificationResult">
    <eStructuralFeatures xsi:type="ecore:EReference" name="target" eType="ecore:EClass platform:/resource/org.osate.verify/model/generated/Verify.ecore#//VerificationActivity"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="validationResult" upperBound="-1"
        eType="#//VerificationResult" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="preconditionResult" upperBound="-1"
        eType="#//VerificationResult" containment="true"/>
  </eClassifiers>
</ecore:EPackage>
