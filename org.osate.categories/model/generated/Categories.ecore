<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="categories" nsURI="http://www.osate.org/categories/Categories"
    nsPrefix="categories">
  <eClassifiers xsi:type="ecore:EClass" name="CategoriesDefinitions">
    <eStructuralFeatures xsi:type="ecore:EReference" name="userCategories" eType="#//Categories"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="phaseCategories" eType="#//Categories"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="qualityCategories" eType="#//Categories"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Categories">
    <eStructuralFeatures xsi:type="ecore:EReference" name="category" upperBound="-1"
        eType="#//Category" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Category">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType platform:/resource/org.eclipse.emf.ecore/model/Ecore.ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="CategoryFilter">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType platform:/resource/org.eclipse.emf.ecore/model/Ecore.ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="userCategory" unique="false"
        upperBound="-1" eType="#//UserCategory"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="anyUserSelection" eType="ecore:EDataType platform:/resource/org.eclipse.emf.ecore/model/Ecore.ecore#//EBoolean"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="qualityCategory" unique="false"
        upperBound="-1" eType="#//QualityCategory"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="anyQualityAttribute" eType="ecore:EDataType platform:/resource/org.eclipse.emf.ecore/model/Ecore.ecore#//EBoolean"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="phaseCategory" unique="false"
        upperBound="-1" eType="#//PhaseCategory"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="anyDevelopmentPhase" eType="ecore:EDataType platform:/resource/org.eclipse.emf.ecore/model/Ecore.ecore#//EBoolean"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="UserCategories" eSuperTypes="#//Categories"/>
  <eClassifiers xsi:type="ecore:EClass" name="QualityCategories" eSuperTypes="#//Categories"/>
  <eClassifiers xsi:type="ecore:EClass" name="PhaseCategories" eSuperTypes="#//Categories"/>
  <eClassifiers xsi:type="ecore:EClass" name="UserCategory" eSuperTypes="#//Category"/>
  <eClassifiers xsi:type="ecore:EClass" name="QualityCategory" eSuperTypes="#//Category"/>
  <eClassifiers xsi:type="ecore:EClass" name="PhaseCategory" eSuperTypes="#//Category"/>
</ecore:EPackage>
