              <subModelOperations xsi:type="tool:SetValue" featureName="name" valueExpression="newreq"/>
              <subModelOperations xsi:type="tool:SetValue" featureName="title" valueExpression="requirement title"/>
            </subModelOperations>
          </firstModelOperation>
        </create>
      </ownedLineMappings>
      <ownedColumnMappings name="NameColumn" headerLabelExpression="Name" featureName="name"/>
      <ownedColumnMappings name="TitleColumn" label="Title" headerLabelExpression="Title" featureName="title"/>
      <ownedColumnMappings name="DescriptionColumn" label="Description" headerLabelExpression="Description" featureName="description" labelExpression="[self.description.description->first().text/]"/>
      <ownedColumnMappings name="GoalColumn" label="Goal" headerLabelExpression="Goal" featureName="goalReference" labelExpression="[self.goalReference.name/]"/>
      <ownedColumnMappings name="RefineColumn" label="Refine" headerLabelExpression="Refines" featureName="refinesReference" labelExpression="[self.refinesReference.name/]"/>
    </ownedRepresentations>
  </ownedViewpoints>
</description:Group>
