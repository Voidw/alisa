        <ownedSubLines name="ReqSpecFolderLine" domainClass="reqSpec.ReqSpecFolder">
          <ownedSubLines name="FolderRequirement" domainClass="reqSpec.Requirement"/>
        </ownedSubLines>
        <create name="CreateRequirement" label="Create Requirement" mapping="//@ownedViewpoints[name='ReqSpecViewPoint']/@ownedRepresentations[name='ReqSpecTable']/@ownedLineMappings[name='Requirements']/@ownedSubLines[name='RequirementLine']">
          <variables name="root" documentation="The semantic element of the table."/>
